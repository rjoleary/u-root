package main

import (
	"log"
	"os"
	"path"
)

func main() {
	name := path.Base(os.Args[0])
	appMain, ok := map string[func ()]{
		{{range _, a := .Applets}}
		.a.Name
		{{end}}
	}[name]
	if !ok {
		log.Fatalf("applet '%s' not found", name)
	}
	appMain()
}
